<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="foodItemRepo" class="com.ezmenutouch.repositories.FoodItemRepo" />
	<bean id="foodCategoryRepo" class="com.ezmenutouch.repositories.FoodCategoryRepo" />
	
	<bean id="itemService" class="com.eztmenutouch.services.PostGresItemService">
		<property name="foodItemRepo" ref="foodItemRepo" />
		<property name="foodCategoryRepo" ref="foodCategoryRepo" />
	</bean>

	
</beans>
